		unique: true
		culture: dwarven
		twilight: 1
		type: condition
		keyword: [
			tale
			support area
		]
		effects: [
			{
				type: extraCost
				cost: {
					type: exert
					select: choose(dwarf)
				}
			}
			{
				type: trigger
