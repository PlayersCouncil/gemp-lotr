			style: Standard
		}
		title: Hollowing of Isengard
		unique: false
		culture: isengard
		twilight: 2
		type: condition
		keyword: support area
		effects: [
		    {
		        type: toPlay
                requires: {
                    type: canSpot
                    filter: culture(isengard),minion
                }
		    }
            {
                type: trigger
                trigger: {
                    type: startofphase
                    phase: Shadow
                }
                optional: true
                effect: {
                    type: costToEffect
                    cost: {
                        type: discardfromhand
                        forced: false
                        select: choose(culture(isengard))
                        count: 1
                        memorize: discardedCard
                    }
                    effect: {
                        type: addTwilight
                        amount: {
                            type: Conditional
                            requires: {
                                type: MemoryMatches
                                memory: discardedCard
                                filter: Orc
                            }
                            true: 5
                            false: 3
                        }
                    }
                }
            }
        ]
		gametext: To play, spot an [isengard] minion.\At the start of your Shadow phase, you may discard an [isengard] card from hand to add (3) (or (5) if that card was an Orc).
		lore: "'Rip them all down!'"
		promotext: ""
		alts: {
			promos: {
			}
			errata: {
			}
		}
	}
	

