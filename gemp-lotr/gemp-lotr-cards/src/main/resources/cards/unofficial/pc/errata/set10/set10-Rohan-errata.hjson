	60_72: {
		cardInfo: {
			//id: 60_72.2
			parent: 10_72
			variant: ERRATA
			variantPath: errata/pc
			revision: 2
			image: errata/LOTR-EN10E072.2_card.jpg
			collInfo: 10R72
			rarity: R
		}
		title: Eowyn
		subtitle: Lady of Ithilien
		unique: true
		side: Free Peoples
		culture: Rohan
		twilight: 2
		type: Companion
		race: Man
		strength: 6
		vitality: 3
		signet: Gandalf
		resistance: 6
		effects: [
			{
				type: activated
				phase: skirmish
				cost: {
					type: exert
					select: self
				}
				effect: [
				{
					type: Repeat
					times: {
						type: forEachWound
						filter: minion,inSkirmishAgainst(self)
					}
					effect: [
						{
							type: ChooseActiveCards
							text: Choose Shadow card to hinder or exert
							count: 1
							player: Shadow
							select: choose(side(shadow))
							memorize: chosenCard
						}
						{
							type: If
							check: {
								type: MemoryMatches
								memory: chosenCard
								filter: CanWound, minion
							}
							true: {
								type: Choice
								player: Shadow
								texts: [
									Wound {chosenCard}
									Hinder {chosenCard}
								]
								effects: [
									{
										type: Wound
										player: Shadow
										select: memory(chosenCard)
									}
									{
										type: Hinder
										player: Shadow
										select: memory(chosenCard)
									}
								]
							}
							false: {
								type: Hinder
								player: Shadow
								select: memory(chosenCard)
							}
						}
					]
				}
			]
			}
		]
		gametext: Skirmish: Exert Eowyn to make an opponent wound or hinder a minion for each wound on each minion skirmishing Eowyn.
		lore: "'Yet now that I have leave to depart, I would remain. For this House has become to me of all dwellings the most blessed.'"
		promotext: ""
		alts: {
			promos: {
			}
			errata: {
			}
		}
	}
	
}