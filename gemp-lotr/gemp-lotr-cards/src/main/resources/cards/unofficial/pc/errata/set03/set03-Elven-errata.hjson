		title: Vilya
		unique: true
		side: Free Peoples
		culture: Elven
		twilight: 1
		type: Artifact
		itemclass: Ring
		target: name(Elrond)
		#keyword: support area
		vitality: 1
		effects: [
			{
				type: Activated
				phase: Maneuver
				requires: {
					type: CanSpot
					filter: condition,side(shadow)
				}
				cost: {
					type: exert
					select: bearer
					times: 2
				}
				effect: [
					{
						type: ChooseActiveCards
						select: choose(condition,side(shadow))
						memorize: chosenShadowCondition
