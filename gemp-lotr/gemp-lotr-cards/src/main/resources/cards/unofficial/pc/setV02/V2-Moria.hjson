						type: MemorizeNumber
						amount: {
							type: ForEachStacked
							filter: not(memory(chosenMinion))
							on: self
							multiplier: -1
						}
						memory: stackedDiscount
					}
					{
						type: PlayCardFromStacked
						select: memory(chosenMinion)
						on: self
						discount: memory(stackedDiscount)
						assumePlayable: true
					}
					{
						type: Discard
						select: choose(self)
					}
				]
			}
