				}
			}
			{
				type: activated
				phase: shadow
				requires: [
					{
						type: HasCardStacked
						filter: playable, unique, culture(moria), minion	
						on: self
					}
				]
				effect: [
					{
						type: ChooseCardsFromSingleStack
						select: choose(playable, unique, culture(moria), minion)
						count: 1
						on: self
						memorize: chosenMinion
						text: Choose a minion to play.
					}
